<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>CalendarSampleServlet</servlet-name>
    <servlet-class>com.google.api.services.samples.calendar.appengine.CalendarSampleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CalendarSampleServlet</servlet-name>
    <url-pattern>/calendar-sampleservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CalendarSampleAuthCallbackServlet</servlet-name>
    <servlet-class>com.google.api.services.samples.calendar.appengine.CalendarSampleAuthCallbackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CalendarSampleAuthCallbackServlet</servlet-name>
    <url-pattern>/oauth2callback</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RoomListServlet</servlet-name>
    <servlet-class>com.dianping.meetingboard.RoomListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoomListServlet</servlet-name>
    <url-pattern>/rooms</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RoomServlet</servlet-name>
    <servlet-class>com.dianping.meetingboard.RoomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoomServlet</servlet-name>
    <url-pattern>/room</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>calendar-sampleservlet</welcome-file>
  </welcome-file-list>

  <!-- The following ensures the user will be logged in to a Google Account by
    redirecting them to the sign-in page if not.
    For more information, see https://developers.google.com/appengine/docs/java/config/webxml -->
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>any</web-resource-name>
      <url-pattern>/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>*</role-name>
    </auth-constraint>
  </security-constraint>

</web-app>
